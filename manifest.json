{
    "manifest_version": 2,
    "name": "Chancer",
    "version": "1.0",
    "description": "Lets the user flip a coin or roll a dice in browser",
    "icons": {
        "32": "icons/dice.png"
    },
    "permissions": ["activeTab"],
    "browser_action": {
        "default_icon": "icons/dice.png",
        "default_title": "Chancer",
        "default_popup": "popup/popup.html"
    }
}